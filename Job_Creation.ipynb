from databricks.sdk import WorkspaceClient

# Define the job as a plain Python dict (not JobSettings)
new_job = {
    "name": "Test_job1",
    "tasks": [
        {
            "task_key": "Training",
            "notebook_task": {
                "notebook_path": "/Workspace/Users/sachin.kumar@snp.com/Training",
                "source": "WORKSPACE",
            },
            "existing_cluster_id": "1124-094650-0nervcma",
        },
        {
            "task_key": "Evaluation",
            "depends_on": [
                {
                    "task_key": "Training",
                },
            ],
            "notebook_task": {
                "notebook_path": "/Workspace/Users/sachin.kumar@snp.com/Evaluation",
                "source": "WORKSPACE",
            },
            "existing_cluster_id": "1124-094650-0nervcma",
        },
    ],
    "queue": {
        "enabled": True,
    },
}

w = WorkspaceClient()

# Here new_job is a dict, so **new_job is valid
result = w.jobs.create(**new_job)
