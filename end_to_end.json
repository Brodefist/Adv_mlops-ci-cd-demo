{
  "name": "Model_Register_Pipeline",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "notification_settings": {
    "no_alert_for_skipped_runs": false,
    "no_alert_for_canceled_runs": false
  },
  "timeout_seconds": 0,
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "Data_Prep",
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Workspace/Users/deni.lakamsani@snp.com/Regression/Data_Prep",
        "source": "WORKSPACE"
      },
      "existing_cluster_id": "0109-065536-lhc8lexu",
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "Train",
      "depends_on": [
        {
          "task_key": "Data_Prep"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Workspace/Users/deni.lakamsani@snp.com/Regression/Training",
        "source": "WORKSPACE"
      },
      "existing_cluster_id": "0109-065536-lhc8lexu",
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "Eval",
      "depends_on": [
        {
          "task_key": "Train"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Workspace/Users/deni.lakamsani@snp.com/Regression/Evaluation",
        "source": "WORKSPACE"
      },
      "existing_cluster_id": "0109-065536-lhc8lexu",
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "Condition-Node",
      "depends_on": [
        {
          "task_key": "Eval"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "condition_task": {
        "op": "EQUAL_TO",
        "left": "{{tasks.Eval.values.passes}}",
        "right": "true"
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "Register_Model",
      "depends_on": [
        {
          "task_key": "Condition-Node",
          "outcome": "true"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Workspace/Users/deni.lakamsani@snp.com/Regression/Register_Model",
        "source": "WORKSPACE"
      },
      "existing_cluster_id": "0109-065536-lhc8lexu",
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "Send-Alert-Mail",
      "depends_on": [
        {
          "task_key": "Condition-Node",
          "outcome": "false"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Workspace/Users/deni.lakamsani@snp.com/Regression/Send_Alert",
        "source": "WORKSPACE"
      },
      "existing_cluster_id": "0109-065536-lhc8lexu",
      "timeout_seconds": 0,
      "email_notifications": {
        "on_success": [
          "deni.lakamsani@snp.com"
        ]
      },
      "notification_settings": {
        "no_alert_for_skipped_runs": false,
        "no_alert_for_canceled_runs": false,
        "alert_on_last_attempt": false
      },
      "webhook_notifications": {}
    }
  ],
  "queue": {
    "enabled": true
  },
  "performance_target": "PERFORMANCE_OPTIMIZED"
}
